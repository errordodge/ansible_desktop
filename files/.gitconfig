[alias]
	aliases = config --get-regexp '^alias\\.'
	delpruned = !git branch --verbose | grep gone | awk '{ print $1 }' | xargs git branch -d
	remoteprune = remote prune origin
[color]
	ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
